<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="48.0" prefWidth="700.0" styleClass="hbox-all" stylesheets="@DMOperational.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DMOperational.DMoperationalController">
   <children>
      <Label prefHeight="17.0" prefWidth="149.0" styleClass="title-label" text="String for encryption:" />
      <TextField fx:id="Encrypt" prefHeight="25.0" prefWidth="179.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#clickDictionaryOptions" prefHeight="27.0" prefWidth="180.0" text="Dictonary options" />
      <Label fx:id="Decrypt" alignment="CENTER" prefHeight="17.0" prefWidth="156.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Button fx:id="Process" mnemonicParsing="false" onAction="#clickProcessButtonAction" prefHeight="27.0" prefWidth="97.0" text="Process">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
   </children>
</HBox>
